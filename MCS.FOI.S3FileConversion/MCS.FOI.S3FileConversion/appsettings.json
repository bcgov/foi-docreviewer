{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EventHub": {
    "Host": "** redis host **",
    "Port": "** redis port **",
    "Password": "** redis password **",
    "StreamKey": "file-conversion"
  },
  "ConversionSettings": {
    "HTMLtoPdfWebkitPath": "C:\\Projects\\foi-docreviewer\\MCS.FOI.S3FileConversion\\MCS.FOI.S3FileConversion\\QtBinariesWindows",
    "FolderSearchPattern": "*",
    "FailureAttemptCount": "10",
    "WaitTimeInMilliSeconds": "5000",
    "DayCountBehindToStart": "6",
    "SyncfusionLicense": "",
    "FileWatcherMonitoringDelayInMilliSeconds": "5000",
    "FileWatcherStartDate": "09-August-2021",
    "CFRArchiveFoldertoSkip": "CFR Archive"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\FOIFileConversion\\Logs\\log-.txt",
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ],
    "Properties": {
      "Application": "FOIFileConversion"
    }
  }
}
